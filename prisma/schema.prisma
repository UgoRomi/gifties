model Merchant {
  id           String  @id
  description  String?
  name         String?
  url          String?
  profileImage String?
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
